# This is an example config file for the squid proxy
http_port <%= @http_port %>
#http_port 8080

#acl localhost src 127.0.0.1
acl whitelist url_regex <%= @acl_whitelist_real %>
#acl whitelist url_regex .naturalis.nl/nl/het-museum/agenda/|.naturalis.nl/media|.naturalis.nl/static/*

acl blacklist dstdomain *
http_access allow whitelist
#http_access deny all

deny_info <%= @deny_info %> whitelist
#deny_info http://www.naturalis.nl/nl/het-museum/agenda/ whitelist

http_reply_access deny blacklist whitelist

# enable caching
cache_peer <%= @cache_peer %> parent 80 0
#cache_peer .naturalis.nl/nl/het-museum/agenda/ parent 80 0