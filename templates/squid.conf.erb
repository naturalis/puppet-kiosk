# This is an example config file for the squid proxy
http_port <%= @http_port %>

#acl localhost src 127.0.0.1
#acl whitelist url_regex <%= @acl_whitelist_real %>
acl whitelist url_regex <%= @acl_whitelist %>

acl blacklist dstdomain *
http_access allow whitelist
#http_access deny all
deny_info <%= @deny_info %> whitelist
http_reply_access deny blacklist whitelist

# enable caching
cache_peer <%= @cache_peer_real %> parent 80 0
cache_mem <%= @cache_mem %>
maximum_object_size <%= @cache_max_object_size %>
maximum_object_size_in_memory <%= @cache_maximum_object_size_in_memory %>