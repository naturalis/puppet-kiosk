# This is an example config file for midori

[settings]
default-encoding=ISO-8859-1
enable-developer-extras=false
enable-site-specific-quirks=true
enable-dns-prefetching=false
enable-javascript=true
default-charset=ISO-8859-1
last-window-width=1024
last-window-height=768
last-window-state=MIDORI_WINDOW_MAXIMIZED
location-entry-search=<%= @homepage %>
show-navigationbar=false
toolbar-items=Back,NextForward
middle-click-opens-selection=false
tabhome=<%= @homepage %>
close-buttons-on-tabs=false
open-new-pages-in=MIDORI_NEW_PAGE_CURRENT
open-tabs-in-the-background=true
open-tabs-next-to-current=false
enable-spell-checking=false
enable-html5-database=true
homepage=<%= @homepage %>
download-folder=/home/kiosk
http-proxy=http://localhost
proxy-type=MIDORI_PROXY_HTTP
user-agent=Mozilla/5.0 (X11; Linux) AppleWebKit/535.22 (KHTML, like Gecko) Chrome/18.0.1025.133 Safari/535.22 Midori/0.5

[extensions]
libaddons.so=true
libmouse-gestures.so=true